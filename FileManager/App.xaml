<Application x:Class="FileManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FileManager"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./View/Themes/ListElement.xaml"/>
                <ResourceDictionary Source="./View/Themes/SearchTextBox.xaml"/>
                <ResourceDictionary Source="./View/Themes/LightTheme.xaml"/>
                <ResourceDictionary Source="./View/Themes/RedTheme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
